{
    "functions": {
      "api/index.ts": {
        "runtime": "nodejs16.x",
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.ts"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  